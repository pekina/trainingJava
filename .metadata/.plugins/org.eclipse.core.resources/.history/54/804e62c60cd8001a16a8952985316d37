/*
 * Perla Huerta
 * 6/Agosto/2020
 * Programa Java que muestre los números del 1 al 100 utilizando la instrucción while
 */
package basicosIterativos;

public class Ejercicio1 {
	public static void main(String[] args) {
		int contador=1;
		
		while(contador <= 100) {
			System.out.println(contador);
			contador++;
		}
	}
}
