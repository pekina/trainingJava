	public static void main(String[] args) {
		Scanner e = new Scanner(System.in);
		int numero;
		System.out.println("Introduzca un número: ");
		numero = e.nextInt();
		
		for (int contador = 0; contador <= numero; contador++)
		{
			for (int fac = contador; fac >= 1; fac--)
			{
				
			}
		}
	}
}
